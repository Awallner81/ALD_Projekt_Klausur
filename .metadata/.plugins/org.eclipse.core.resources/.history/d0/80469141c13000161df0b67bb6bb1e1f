
public class HelloWorld3 {

	public static void main(String[] args) {

		// TODO Auto-generated method stub

		
		
		
		int switchValue =2;
		
		switch(switchValue){
		
		case 1:
			System.out.println("Value was 1");
			break;
		case 2:
			System.out.println("Value was 2");
			break;
			
		default:
			System.out.println("was not 1 or 2");
			break;
		}
		
	}

}
